{
  "name": "random-stuff",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "devt": "next dev --turbopack",
    "debug": "next dev",
    "build": "ts-node lib/backend/node/downloadSymbolCompanies.ts & next build",
    "start": "next start",
    "lint": "next lint",
    "download": "ts-node lib/backend/node/downloadAnimals.ts | ts-node lib/backend/node/downloadSymbolCompanies.ts",
    "downloadandbuild": "npx ts-node --project tsconfig-dev.json lib/backend/node/downloadAnimals.ts | next build",
    "test": "jest",
    "build-docker": "ts-node build/docker/generate-config-files.ts && docker build --secret id=CRON_SECRET,src=build/docker/config/CRON_SECRET.txt --secret id=NEXT_PUBLIC_APP_ID,src=build/docker/config/NEXT_PUBLIC_APP_ID.txt --secret id=NEXT_PUBLIC_QLN_API_URL,src=build/docker/config/NEXT_PUBLIC_QLN_API_URL.txt --secret id=NEXT_PUBLIC_API_TOKEN,src=build/docker/config/NEXT_PUBLIC_API_TOKEN.txt --secret id=NEXT_QLN_API_PUBLIC_KEY,src=build/docker/config/NEXT_QLN_API_PUBLIC_KEY.txt --secret id=CONTENTFUL_GRAPH_BASE_URL,src=build/docker/config/CONTENTFUL_GRAPH_BASE_URL.txt --secret id=CONTENTFUL_ACCESS_TOKEN,src=build/docker/config/CONTENTFUL_ACCESS_TOKEN.txt --secret id=CONTENTFUL_SPACE_ID,src=build/docker/config/CONTENTFUL_SPACE_ID.txt --secret id=AWS_ACCESS_KEY_ID,src=build/docker/config/AWS_ACCESS_KEY_ID.txt --secret id=AWS_ACCESS_KEY_SECRET,src=build/docker/config/AWS_ACCESS_KEY_SECRET.txt --secret id=ANTHROPIC_API_KEY,src=build/docker/config/ANTHROPIC_API_KEY.txt -t random-stuff .",
    "run-docker": "docker run -d --name random-stuff-container -p 3000:3000 random-stuff",
    "docker-stop": "docker stop random-stuff-container",
    "docker-remove": "docker rm random-stuff-container",
    "docker-clean-all": "powershell -ExecutionPolicy Bypass -File cleanup-containers.ps1",
    "docker-clean-build-run": "yarn docker-clean-all && yarn build-docker && docker run -d --name random-stuff-container -p 3000:3000 random-stuff",
    "docker-build-run": "yarn build-docker && docker run -d --name random-stuff-container -p 3000:3000 random-stuff"
  },
